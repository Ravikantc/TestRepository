<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE DynamicScope PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<DynamicScope name="Help Desk">
  <ApplicationRequestControl>
    <Reference class="sailpoint.object.Rule" name="All Objects"/>
  </ApplicationRequestControl>
  <Description/>
  <ManagedAttributeRequestControl>
    <Reference class="sailpoint.object.Rule" name="All Objects"/>
  </ManagedAttributeRequestControl>
  <PopulationRequestAuthority allowAll="true" ignoreScoping="true"/>
  <RoleRequestControl>
    <Reference class="sailpoint.object.Rule" name="WSI Filter Birthright Roles Rule"/>
  </RoleRequestControl>
  <Selector>
    <IdentitySelector>
      <MatchExpression>
        <MatchTerm name="capabilities" value="HelpDesk"/>
      </MatchExpression>
    </IdentitySelector>
  </Selector>
</DynamicScope>
